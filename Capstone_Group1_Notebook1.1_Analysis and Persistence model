    "# Read in data set \n",
    "dataset = pd.read_csv('DataScienceCourse_EDPdata.csv', \n",
    "                  sep=';')\n",
    "\n",
    "\n",
    "#Remove excess timestamp data\n",
    "dataset.Timestamp = dataset.Timestamp.str.replace('\\+00:00', '')"
   ]
  },
