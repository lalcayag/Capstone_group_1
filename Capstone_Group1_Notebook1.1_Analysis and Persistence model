   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Rename all windspeed data to W using mean, rename all power data to P summing up power\n",
    "P = dataset_ch.Grd_Prod_Pwr_Avg.groupby(level=0).sum()\n",
    "W = dataset_ch.Amb_WindSpeed_Avg.groupby(level=0).mean()\n",
    "\n",
    "P_turb_01 = dataset_ch.loc[dataset_ch.Turbine_ID =='T01'].Grd_Prod_Pwr_Avg\n",
    "P_turb_diff_01 = dataset_ch.loc[dataset_ch.Turbine_ID=='T01'].Grd_Prod_Pwr_Avg.diff()\n",
    "\n",
    "P_diff = dataset_ch.Grd_Prod_Pwr_Avg.groupby(level=0).sum().diff()"
   ]
