   "source": [
    "# Persistance model 1 hour, T01\n",
    "sampling_rate = '60min' # we have 10min resolution in our dataset\n",
    "resampling_rate = '10T' # we have 10min resolution in our dataset\n",
    "n_lag = 6\n",
    "persistance=None\n",
    "persistance = pd.DataFrame(y)\n",
    "columns = ['var(t)']\n",
    "for i in range(1,n_lag):  \n",
    "    t01_60 = pd.DataFrame(y[i:].resample(sampling_rate).asfreq())  \n",
    "    t01_60 = t01_60.resample(resampling_rate).asfreq()\n",
    "    t01_60.ffill(inplace=True)\n",
    "    columns+=['var' + '(t+' + str(i)+')']\n",
    "    persistance = pd.concat([persistance, t01_60],axis=1)\n",
    "persistance.columns = columns\n",
    "real_values = forecast_sequences_output(persistance['var(t)'],n_lag)\n",
    "real_values.columns = columns\n",
    "real_values.head(10),persistance.head(10)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    " <span style=\"color:blue\">  Now we have our persistence dataset and a dataset with 5 columns of real data to compare both datasets."
   ]
  },
